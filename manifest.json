{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/bidicon.png"
   },
   "content_scripts": [ {
      "js": [ "jquery-3.2.1.min.js" ],
      "matches": [ "\u003Call_urls>" ],
      "default_icon": "images/icons/favicon.png"
   } ],
   "icons": { 
      "16": "images/icons/16x16.png",
      "48": "images/icons/48x48.png",
      "128": "images/icons/128x128.png"
   },
   "description": "Allows entering prices directly",
   "externally_connectable": {
      "matches": [ "*://*.iauc.co.jp/*", "https://sayuri.co.jp/", "http://oliac.com/" ]
   },
   "manifest_version": 2,
   "name": "ITOL DLM",
   "options_page": "options.html",
   "permissions": [ "tabs", "activeTab", "https://*.iauc.co.jp/*", "http://*.asnet2.com/*", "cookies", "storage" ],
   "short_name": "DLM",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   "web_accessible_resources": [
    "images/*",
    "js/*",
    "css/*"
  ]
}
